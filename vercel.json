{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1",
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/index.html"
    }
  ]
} 